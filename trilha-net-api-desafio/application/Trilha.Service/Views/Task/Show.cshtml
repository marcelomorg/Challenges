@model IEnumerable<Trilha.Model.Models.Tarefa>

<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th scope="col">@Html.DisplayNameFor(model => model.Id)</th>
      <th scope="col">@Html.DisplayNameFor(model => model.Titulo)</th>
      <th scope="col">@Html.DisplayNameFor(model => model.Descricao)</th>
      <th scope="col">@Html.DisplayNameFor(model => model.Data)</th>
      <th scope="col">@Html.DisplayNameFor(model => model.Status)</th>
    </tr>
  </thead>
  <tbody>
    @foreach ( var item in Model)
    {
      <tr>
        <th scope="row">@Html.DisplayFor(model => item.Id)</th>
        <td>@Html.DisplayFor(model => item.Titulo)</td>
        <td>@Html.DisplayFor(model => item.Descricao)</td>
        <td>@Html.DisplayFor(model => item.Data)</td>
        <td>@Html.DisplayFor(model => item.Status)</td>
      </tr>
    }
  </tbody>
</table>